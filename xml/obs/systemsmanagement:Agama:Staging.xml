<openQA
    project_pattern="systemsmanagement:Agama:Staging"
    dist_path="obspublish-other::openqa/systemsmanagement:Agama:Staging/"
    distri="alp"
    version="agama-2.1-staging"
    archs="x86_64 aarch64 ppc64le s390x">
    <flavor name="agama-live-ALP" folder="images/*/agama-live:ALP">
        <hdd filemask="agama-live.*-ALP-Build.*iso$"/>
    </flavor>
    <flavor name="agama-live-default" folder="images/*/agama-live:default">
        <hdd filemask="agama-live.*-default-Build.*iso$"/>
    </flavor>
    <flavor name="agama-live-default-Playwright" folder="images/*/agama-live:default-Playwright">
        <hdd filemask="agama-live.*-default-Playwright-Build.*iso$"/>
    </flavor>
</openQA>
